{
  "name": "vuetifyvue",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "description": "Template for setting up Vuetify + Vue with Vite and GitHub Pages",
  "homepage": "https://<USERNAME>.github.io/VuetifyVue",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:local": "vue-tsc -b && vite build --base ./ && cp -f docs/index.html docs/404.html",
    "preview": "vite preview"
  },
  "dependencies": {
    "sass": "^1.94.0",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "vuetify": "3.11.0-beta.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.2",
    "vite-plugin-md": "^0.21.5",
    "vite-plugin-pages": "^0.33.1",
    "vue-tsc": "^3.1.3"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.2"
    }
  }
}
